```{r export, echo=FALSE, message=TRUE, warning=TRUE, include=FALSE}

variables_with_labels = as.data.frame(bern_data_long2)  %>% map(function(x) attr(x, "class") == "haven_labelled") %>% map(.,1) %>% #added map(.,1)
  unlist() %>%
  names()

variables_with_labels = variables_with_labels[which(variables_with_labels!="START_DATE" & variables_with_labels!="END_DATE" )]

bern_data_long2_factored = bern_data_long2 %>%
  mutate_at( vars(variables_with_labels[2:length(variables_with_labels)]), as_factor)


#%>% select(-c(zuf_fam,zuf_par,zuf_ber,zuf_frz)) 
#data incl missings
write_dta(as.data.frame(bern_data_long2_factored), "C:/Users/Kalleitner/Documents/R/W1-35_CORONAPANEL_recoded_inc_miss.dta")
saveRDS(bern_data_long2_factored,"C:/Users/Kalleitner/Documents/R/W1-35_CORONAPANEL_recoded_inc_miss.rds")

#write_dta(bern_data_long2_factored, "./../../data/processed/W1-26_CORONAPANEL_recoded_inc_miss.dta")
#saveRDS(bern_data_long2_factored,"./../../data/processed/W1-26_CORONAPANEL_recoded_inc_miss.rds")

#data excl missings
bern_data_long2_factored <- as.data.frame(bern_data_long2_factored) %>% mutate_if(is.factor, ~(na_if(.,"Weiss nicht")))
bern_data_long2_factored <- as.data.frame(bern_data_long2_factored) %>% mutate_if(is.factor, ~(na_if(.,"keine Angabe")))
bern_data_long2_factored <- as.data.frame(bern_data_long2_factored) %>% mutate_if(is.factor, ~(na_if(.,"keine Angabe2")))

write_dta(as.data.frame(bern_data_long2_factored), "C:/Users/Kalleitner/Documents/R/W1-35_CORONAPANEL_recoded.dta")
saveRDS(bern_data_long2_factored,"C:/Users/Kalleitner/Documents/R/W1-35_CORONAPANEL_recoded.rds")

#write_dta(bern_data_long2_factored, "./../../data/processed/W1-26_CORONAPANEL_recoded.dta")
#saveRDS(bern_data_long2_factored,"./../../data/processed/W1-26_CORONAPANEL_recoded.rds")

names(bern_data_long2_factored)[!(names(bern_data_long2_factored)==gsub("-|\\.|\\/|'|\\[|\\]","",names(bern_data_long2_factored)))]
