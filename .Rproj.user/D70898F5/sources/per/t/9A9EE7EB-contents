#Umgang mit Corona
bern_data_ren3<-bern_data_ren3 %>% rename(W20_umgcor_hart=W20_Q23A1,  #Wir sollten möglichst harte Maßnahmen für eine kürzere Krise in Kauf nehmen.
                                          W20_umgcor_zero=W20_Q23A2,	#Man sollte Maßnahmen erst lockern, wenn Corona-Neuansteckungen bei Null sind.
                                          W20_umgcor_impf=W20_Q23A3,	#Sobald Risikogruppen geimpft sind, sollte alles normal geöffnet werden.
                                          W20_umgcor_eigv=W20_Q23A4,	#Wir sollten alle Beschränkungen aufheben, dafür auf Eigenverantwortung setzen.
                                          W20_umgcor_lock=W20_Q23A5,	#Maßnahmen sollen gelockert werden, auch wenn das mehr Corona-Infizierte bedeutet.
                                          
                                          W28_umgcor_hart=W28_Q23A1,  #Wir sollten möglichst harte Maßnahmen für eine kürzere Krise in Kauf nehmen.
                                          W28_umgcor_zero=W28_Q23A2,	#Man sollte Maßnahmen erst lockern, wenn Corona-Neuansteckungen bei Null sind.
                                          
                                          W28_umgcor_eigv=W28_Q23A3,	#Wir sollten alle Beschränkungen aufheben, dafür auf Eigenverantwortung setzen.
                                          W28_umgcor_lock=W28_Q23A4)	#Maßnahmen sollen gelockert werden, auch wenn das mehr Corona-Infizierte bedeutet.



