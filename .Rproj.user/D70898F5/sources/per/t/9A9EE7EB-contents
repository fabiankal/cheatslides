bern_data_exportlab<-bern_data_ren3_test

#change avoiding answer in satisfaction question to NA
bern_data_exportlab$W5_zuf_fam[bern_data_exportlab$W5_zuf_fam==0]<-NA
bern_data_exportlab$W5_zuf_par[bern_data_exportlab$W5_zuf_par==0]<-NA
bern_data_exportlab$W5_zuf_ber[bern_data_exportlab$W5_zuf_ber==0]<-NA
bern_data_exportlab$W5_zuf_frz[bern_data_exportlab$W5_zuf_frz==0]<-NA

#get label attribute from dataframe
labels <- sapply(bern_data_exportlab %>% select(variablelist), function(x) attr(x, "label"))
      tibble(name = names(labels),
             label = labels)
      
#convert to dataframe cut wave prefix and export variable labels
labeldf <- as.data.frame(labels) %>% mutate(variable=str_remove(rownames(.),"W\\d+_")) 
      
#get minimum and maximum of variables
labels2 <- bern_data_exportlab %>% ungroup() %>% select(variablelist) %>%  sapply(function(x) attr(x, "labels"))

df = as.data.frame(do.call(rbind, lapply(labels2, unlist))) %>% na_if(.,c(77)) %>% na_if(.,c(88)) %>% na_if(.,c(99)) 

df2 <- df %>% mutate(min = pmap(df, min,na.rm=TRUE), 
                     max = pmap(df, max,na.rm=TRUE)) %>% mutate(variable=str_remove(rownames(.),"W\\d+_")) 

#combine variable labels and min/max
labeldfcombined <- labeldf %>% left_join(df2 %>% select(variable,min,max), by="variable") %>% 
  na_if(., "NULL") %>% mutate(min=as.integer(min),
                              max=as.integer(max))

labeldfcombined$min[labeldfcombined$variable=="zuf_fam"]=labeldfcombined$min[labeldfcombined$variable=="zuf_fam"]+1
labeldfcombined$min[labeldfcombined$variable=="zuf_par"]=labeldfcombined$min[labeldfcombined$variable=="zuf_par"]+1
labeldfcombined$min[labeldfcombined$variable=="zuf_ber"]=labeldfcombined$min[labeldfcombined$variable=="zuf_ber"]+1
labeldfcombined$min[labeldfcombined$variable=="zuf_frz"]=labeldfcombined$min[labeldfcombined$variable=="zuf_frz"]+1

#write_csv(labeldfcombined, "./../../data/processed/varlabels.csv", na = "NA")
write_csv(labeldfcombined, "./data/processed/varlabels.csv", na = "NA")
write_csv(labeldfcombined, "C:/Users/Kalleitner/Documents/GIT/woco/output/tables/varlabels.csv", na = "NA")

